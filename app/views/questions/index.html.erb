<h1>All Questions
<%= link_to "New", new_question_path(@question), class: 'btn btn-success' %>
</h1>
<% @questions.each do |question| %>
  <div class="media">
   <div class="media-body">
     <h4 class="media-heading">
       <%= link_to question.title, question %>
     </h4>
   </div>
 </div>
<% end %>
